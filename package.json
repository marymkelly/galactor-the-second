{
  "name": "galactor2",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.js",
  "scripts": {
    "start": "node -r dotenv/config src/server.js dotenv_config_path=./config/dev.env",
    "dev-server": "nodemon -r dotenv/config src/server.js dotenv_config_path=./config/dev.env --exec babel-node",
    "sass:compile": "sass --load-path sass src/public/css/scss/styles.scss src/public/css/styles.css",
    "watch-sass": "sass src/public/css/scss/styles.scss src/public/css/styles.css --watch",
    "test": "NODE_ENV=test jest --config=jest.config.json --watch",
    "dev": "concurrently --kill-others-on-fail \"npm run watch-sass\" \"npm run dev-server\""
  },
  "nodemonConfig": {
    "ignore": [
      "*.test.js",
      "./config/playground.js"
    ],
    "ext": "js,mjs,json,css"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "axios": "^0.21.0",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.5",
    "express": "^4.17.1",
    "firebase": "^8.2.6",
    "socket.io": "^2.3.0",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "babel-jest": "^26.6.3",
    "concurrently": "^5.3.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "sass": "^1.28.0",
    "superagent": "^6.1.0",
    "supertest": "^6.0.1"
  }
}
